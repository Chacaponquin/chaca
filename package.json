{
  "name": "chaca",
  "version": "1.0.1",
  "description": "A Node JS library to create and export data schemas with your rules",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/types/index.d.ts"
    }
  },
  "files": [
    "lib/**/*",
    "LICENSE",
    "CONTRIBUTING.md",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run fix && npm run prettier-format && npm run build:lib",
    "build:lib": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:types": "tsc -p ./tsconfig.types.json",
    "build:esm": "tsc --p tsconfig.esm.json",
    "build:cjs": "tsc --p tsconfig.cjs.json",
    "test": "jest",
    "prettier-format": "prettier --config .prettierrc src/**/*.ts --write",
    "fix": "eslint --fix src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chacaponquin/ch-data.git"
  },
  "keywords": [
    "chaca",
    "fake-data",
    "generate data",
    "random data",
    "data",
    "fake data",
    "fake-data-generator",
    "logical data"
  ],
  "author": "chacaponquin <hectorangel2001@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/jest": "^29.2.2",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^18.8.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-file-extension-in-import-ts": "^1.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.11.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "json2csv": "^5.0.7",
    "lorem-ipsum": "^2.0.8"
  },
  "bugs": {
    "url": "https://github.com/Chacaponquin/ch-data/issues"
  },
  "homepage": "https://github.com/Chacaponquin/ch-data#readme"
}
